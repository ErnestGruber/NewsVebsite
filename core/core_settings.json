{
  "logger": {
  "levels": {
    "error": 0,
    "warn": 1,
    "info": 2,
    "http": 3,
    "debug": 4
  },
  "format": {
    "timestamp": "YYYY-MM-DD HH:mm:ss",
    "label": "app",
    "level": true,
    "message": true
  },
  "transports": [
    {
      "type": "console",
      "level": "debug",
      "format": "simple"
    },
    {
      "type": "file",
      "filename": "logs/app.log",
      "level": "info",
      "format": "json"
    }
  ]
}

}